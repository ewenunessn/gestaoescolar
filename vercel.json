{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "installCommand": "cd api && npm install",
  "buildCommand": "cd backend && npm run build",
  "outputDirectory": "backend/dist"
}
