{
  "name": "alimentacao-escolar-backend",
  "version": "1.0.0",
  "description": "Backend do Sistema de Gerenciamento de Alimentação Escolar",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc --skipLibCheck",
    "vercel-build": "npm run build && npm run vercel-post-build",
    "start": "node dist/index.js",
    "start:vercel": "node api/index.js",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:tenant": "ts-node tests/runAllTests.ts",
    "test:tenant:coverage": "ts-node tests/runAllTests.ts --coverage",
    "test:tenant:unit": "ts-node tests/runAllTests.ts --suite unit",
    "test:tenant:integration": "ts-node tests/runAllTests.ts --suite integration",
    "test:tenant:performance": "ts-node tests/runAllTests.ts --suite performance",
    "test:tenant:security": "ts-node tests/runAllTests.ts --suite security",
    "migrate": "node run-migration.js",
    "migrate:guias": "node run-migration-guias.js",
    "migrate:guias:local": "node run-migration-guias-local.js",
    "migrate:tenant": "node run-tenant-migration.js",
    "migrate:tenant-id": "node run-add-tenant-id-migration.js",
    "migrate:rls": "node run-rls-migration.js",
    "test:tenant-isolation": "node test-tenant-isolation.js",
    "fix:rls": "node run-fix-rls.js",
    "migrate:tenant-system": "node run-tenant-migrations.js",
    "init:tenant-migrations": "node init-tenant-migration-system.js",
    "test:tenant-migrations": "node test-tenant-migration-system.js",
    "optimize:performance": "node run-tenant-performance-optimization.js",
    "test:performance": "node test-tenant-performance.js",
    "test:performance-simple": "node test-tenant-performance-simple.js",
    "vercel-post-build": "echo '✅ Build concluído - Arquivos compilados em dist/' && ls -la dist/ || dir dist/"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/redis": "^4.0.10",
    "@types/socket.io": "^3.0.1",
    "@types/xlsx": "^0.0.35",
    "@vercel/node": "^3.0.0",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "cron": "^4.3.3",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "redis": "^5.9.0",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/exceljs": "^0.5.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.24",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.56.0",
    "jest": "^30.0.4",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.4"
  }
}
