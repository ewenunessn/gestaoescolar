{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "https://gestaoescolar-xtu1-git-main-ewenunes0-4923s-projects.vercel.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "env": {
    "VITE_NODE_ENV": "production",
    "VITE_VERCEL": "true",
    "VITE_API_URL": "https://gestaoescolar-xtu1-git-main-ewenunes0-4923s-projects.vercel.app/api",
    "VITE_HEALTH_URL": "https://gestaoescolar-xtu1-git-main-ewenunes0-4923s-projects.vercel.app/health"
  }
}